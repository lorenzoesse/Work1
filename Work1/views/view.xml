<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="course_form_view">
            <field name="name">book.form</field>
            <field name="model">book</field>
            <field name="arch" type="xml">
                <form string="Book Form">
                    <sheet>
                        <group>
                            <field name="bookName"/>
                            <field name="publishYear"/>
                            <field name="isbnCode"/>
                        </group>
                    </sheet>
                </form>
           </field>
        </record>
    </data>
</odoo>